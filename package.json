{
  "name": "acis",
  "version": "1.0.0",
  "author": "dyw-design <1875742887@qq.com>",
  "description": "acis",
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "node .electron-vue/build.js && electron-builder",
    "build:schedule": "cross-env BUILD_TARGET=schedule node .electron-vue/build.js && electron-builder",
    "build:statistics": "cross-env BUILD_TARGET=statistics node .electron-vue/build.js && electron-builder",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev_local": "concurrently \"nodemon ./mock/server.js\" \"node .electron-vue/dev-runner.js\"",
    "dev:schedule": "cross-env BUILD_TARGET=schedule node .electron-vue/dev-runner.js",
    "dev:statistics": "cross-env BUILD_TARGET=statistics node .electron-vue/dev-runner.js",
    "dev": "node .electron-vue/dev-runner.js",
    "dev:no-mock": "cross-env MOCK=none node .electron-vue/dev-runner.js",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "postinstall": "npm run lint:fix",
    "mock": "node ./mock/server.js"
  },
  "build": {
    "productName": "acis",
    "appId": "acis",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/favicon.ico"
    },
    "linux": {
      "icon": "build/icons"
    },
    "asar": true,
    "nsis": {
      "oneClick": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "build/icons/favicon.ico",
      "uninstallerIcon": "build/icons/favicon.ico",
      "installerHeaderIcon": "build/icons/favicon.ico",
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "acis"
    }
  },
  "dependencies": {
    "axios": "^0.18.0",
    "chalk": "^2.4.2",
    "echarts": "^4.8.0",
    "element-ui": "^2.13.2",
    "json-server": "^0.16.1",
    "lodash": "^4.17.20",
    "mockjs": "^1.1.0",
    "moment": "^2.26.0",
    "normalize.css": "^8.0.1",
    "resize-detector": "^0.2.2",
    "sass": "^1.26.10",
    "socket.io": "^2.3.0",
    "sortablejs": "^1.10.2",
    "spritejs": "^3.7.19",
    "vue": "^2.6.12",
    "vue-contextmenu": "^1.5.10",
    "vue-contextmenujs": "^1.3.9",
    "vue-electron": "^1.0.6",
    "vue-router": "^3.0.1",
    "vuedraggable": "^2.24.0",
    "vuex": "^3.0.1",
    "vuex-electron": "^1.0.0",
    "vxe-table": "^2.9.17",
    "vxe-table-plugin-element": "^1.8.9",
    "xe-utils": "^2.7.8"
  },
  "devDependencies": {
    "ajv": "^6.5.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "cfonts": "^2.1.2",
    "chalk": "^2.4.2",
    "concurrently": "^5.2.0",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.6",
    "css-loader": "^0.28.11",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "^6.0.1",
    "electron-builder": "^21.2.0",
    "electron-debug": "^3.0.1",
    "electron-devtools-installer": "^2.2.4",
    "eslint": "^6.2.2",
    "eslint-config-standard": "^14.1.1",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-html": "^4.0.3",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.2.2",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "0.4.0",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "node-sass": "^4.14.1",
    "pug": "^3.0.0",
    "pug-html-loader": "^1.1.5",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "url-loader": "^1.0.1",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^15.2.4",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-merge": "^4.1.3"
  },
  "__npminstall_done": false
}
